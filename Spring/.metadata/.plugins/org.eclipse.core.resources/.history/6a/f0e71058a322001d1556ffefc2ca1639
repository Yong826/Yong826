		// alert("하이맨")
		// 파일 업로드 관련 로직 처리
		// .jsp의 form 태그를 대채 (FormData함수)
		let formData = new FormData();
		
		let inputFile = $("input[name='uploadFile']");
		
		let files = inputFile[0].files;
		
		console.log(files);
		
		for(let i=0; files.lenght; i++){
			formData.append("uploadFile", filespi[i]);
		}
	})
})